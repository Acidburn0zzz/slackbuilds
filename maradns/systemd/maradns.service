[Unit]
Description=A DNS server
After=syslog.target network.target local-fs.target
ConditionFileIsExecutable=/etc/rc.d/rc.maradns

[Service]
ExecStop=/etc/rc.d/rc.maradns stop
ExecStart=/etc/rc.d/rc.maradns start
ExecReload=/etc/rc.d/rc.maradns restart
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
