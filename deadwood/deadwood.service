[Unit]
Description=Deadwood recursive caching DNS resolver
After=syslog.target network.target local-fs.target
ConditionFileIsExecutable=/etc/rc.d/rc.deadwood

[Service]
Type=forking
ExecStop=/etc/rc.d/rc.deadwood stop
ExecStart=/etc/rc.d/rc.deadwood start
ExecReload=/etc/rc.d/rc.deadwood restart
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
