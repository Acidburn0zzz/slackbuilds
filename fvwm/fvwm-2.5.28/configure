# Look harder for a C preprocessor for FvwmCpp
# Extract the first word of "cpp", so it can be a program name with args.
set dummy cpp; ac_word=$2
{ echo "$as_me:$LINENO: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6; }
if test "${ac_cv_path_FVWM_CPP+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  case $FVWM_CPP in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_FVWM_CPP="$FVWM_CPP" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
as_dummy="$PATH:/lib:/usr/lib:/usr/ccs/lib"
for as_dir in $as_dummy
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_FVWM_CPP="$as_dir/$ac_word$ac_exec_ext"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done
IFS=$as_save_IFS

  test -z "$ac_cv_path_FVWM_CPP" && ac_cv_path_FVWM_CPP="no"
  ;;
esac
