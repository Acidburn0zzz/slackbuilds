  --with-imagepath=PATH   colon-delimited search path for images
  --with-pkgconfig-prefix=PFX
                          prefix where pkg-config is installed
  --with-pkgconfig-exec-prefix=PFX
                          exec prefix where pkg-config is installed
  --with-freetype-prefix=PFX
