  --with-fontconfig-prefix=PFX
                          prefix where fontconfig is installed (for Xft2)
  --with-fontconfig-exec-prefix=PFX
                          exec prefix where fontconfig is installed
  --with-xft-prefix=PFX   prefix where Xft2 is installed (optional)
  --with-xft-exec-prefix=PFX
                          exec prefix where Xft2 is installed
  --with-xpm-library=PATH use xpm library (for coloured or shaped icons)
  --with-xpm-includes=DIR set directory for xpm headers
  --with-png-library=PATH use png library (for coloured or shaped icons)
  --with-png-includes=DIR set directory for png headers
  --with-rplay-library=PATH
                          use rplay library (adds audio capability)
  --with-rplay-includes=DIR
